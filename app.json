{
  "expo": {
    "name": "SurveyPlus-brand",
    "slug": "SurveyPlus-brand",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0059D4"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.saumya.SurveyPlusBrand",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0059D4"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
